package aula;

public class num_imp {

	public static void main(String[] args) {
		int soma = 0;
		
		for (int i = 1; i <= 100; i += 2 ) {
		System.out.println(i);
}
		
		for (int i = 1; i < 100; i++) {
			soma += i;
}
		
		for (int i = 138; i <= 50000; i++) {
			soma = soma + i;
			System.out.println(i);
		}
		System.out.println(soma);
	}
}
